{
  "name": "webpack-s3-plugin",
  "version": "1.2.0-rc.0",
  "description": "Uploads compiled assets to s3 after build",
  "main": "dist/s3_plugin.js",
  "scripts": {
    "build": "webpack --mode production",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "test": "NODE_ENV='src/test' mocha -t 10000 --require @babel/register",
    "lint": "eslint ./src src/test",
    "watch": "webpack --watch --mode development",
    "prep:major": "npm run build && npm version major",
    "prep:minor": "npm run build && npm version minor",
    "prep:patch": "npm run build && npm version patch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:webpack-contrib/s3-plugin-webpack.git"
  },
  "keywords": [
    "s3",
    "webpack",
    "node",
    "upload",
    "production"
  ],
  "author": "Mika Kalathil",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webpack-contrib/s3-plugin-webpack/issues"
  },
  "homepage": "https://github.com/webpack-contrib/s3-plugin-webpack",
  "dependencies": {
    "aws-sdk": "^2.819.0",
    "lodash": "^4.17.20",
    "mime": "^2.4.7",
    "progress": "^2.0.3",
    "recursive-readdir": "^2.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/preset-env": "^7.12.0",
    "@babel/register": "^7.12.10",
    "babel-loader": "^8.2.0",
    "@types/node": "14.14.17",
    "@types/jest": "26.0.19",
    "jest": "26.6.3",
    "ts-jest": "26.4.4",
    
    "css-loader": "^5.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.12.1",
    "eslint-loader": "^4.0.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.2.1",
    
    "style-loader": "^2.0.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.3.0",
    "typescript": "^4.1.3",
    "shelljs": "^0.8.4"
  },
  
  "peerDependencies": {
    "webpack": ">=4.0.0"
  },
  "resolutions": {
    "@types/node": "14.14.17",
    "@types/jest": "26.0.19",
    "jest": "26.6.3",
    "ts-jest": "26.4.4"
  }
}
